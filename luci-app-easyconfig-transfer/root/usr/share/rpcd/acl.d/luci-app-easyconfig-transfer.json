{
	"luci-app-easyconfig-transfer": {
		"description": "Grant access to LuCI app easyconfig transfer",
		"read": {
			"cgi-io": [ "exec", "download" ],
			"ubus": {
				"luci-rpc": [ "getDHCPLeases" ],
				"file": [ "read" ]
			},
			"file": {
				"/bin/cp": [ "exec" ],
				"/bin/lock": [ "exec" ],
				"/bin/mkdir": [ "exec" ],
				"/bin/rm": [ "exec" ],
				"/bin/gzip -t /tmp/easyconfig_statistics.json.gz": [ "exec" ],
				"/bin/gzip -d /tmp/easyconfig_statistics.json.gz": [ "exec" ],
				"/bin/gzip -k /tmp/easyconfig_statistics.json": [ "exec" ],
				"/usr/bin/easyconfig_statistics.uc": [ "exec" ],
				"/usr/bin/easyconfig_statistics.sh": [ "exec" ],
				"/usr/bin/auto_reset_statistics.sh": [ "exec" ],
				"/sbin/transfer2cron.sh": [ "exec" ],
				"/sbin/backup2cron.sh": [ "exec" ],
				"/sbin/ar2cron.sh": [ "exec" ],
				"/tmp/easyconfig_statistics.json": [ "read" ],
				"/tmp/easyconfig_statistics_gz_tmp.json": [ "read" ],
				"/tmp/dhcp.leases": [ "read" ],
				"/usr/lib/easyconfig/easyconfig_statistics.json.gz": [ "read" ],
				"/usr/lib/easyconfig/easyconfig_statistics.json": [ "read" ],
				"/var/lock/easyconfig_statistics.lock": [ "read" ],
				"/etc/config/easyconfig_transfer": [ "read" ],
				"/mnt/easyconfig_statistics.json": [ "read" ]
			},
			"uci": [ "easyconfig_transfer" ]
		},
		"write": {
			"cgi-io": [ "exec", "upload" ],
			"file": {
				"/bin/cp": [ "exec" ],
				"/bin/mkdir": [ "exec" ],
				"/bin/rm": [ "exec" ],
				"/usr/bin/easyconfig_statistics.uc": [ "exec" ],
				"/usr/bin/easyconfig_statistics.sh": [ "exec" ],
				"/usr/bin/auto_reset_statistics.sh": [ "exec" ],
				"/sbin/transfer2cron.sh": [ "exec" ],
				"/sbin/backup2cron.sh": [ "exec" ],
				"/sbin/ar2cron.sh": [ "exec" ],
				"/tmp/easyconfig_statistics.json": [ "write" ],
				"/tmp/easyconfig_statistics.json.gz": [ "write" ],
				"/tmp/easyconfig_statistics_gz_tmp.json": [ "write" ],
				"/usr/lib/easyconfig/easyconfig_statistics.json.gz": [ "write" ],
				"/usr/lib/easyconfig/easyconfig_statistics.json": [ "write" ],
				"/var/lock/easyconfig_statistics.lock": [ "write" ],
				"/etc/config/easyconfig_transfer": [ "write" ],
				"/mnt/easyconfig_statistics.json": [ "write" ]
			},
			"ubus": {
				"file": [ "remove", "write" ]
			},
			"uci": [ "easyconfig_transfer" ]
		}
	}
}
